@model BitirmeTeziKerim.Models.Sayilar

@{
    ViewData["Title"] = "Yarat";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>Yeni Liste Oluştur</h1>

<h4>Sayilar</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="SayiTablo" class="control-label"></label>
                <input asp-for="SayiTablo" class="form-control" />
                <span asp-validation-for="SayiTablo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Ekle" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Listeye Dön</a>
</div>

